<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">

<form method="post" >
    <input class="w-75 rounded-4  p-2 d-block m-auto mt-1 border border-secondary-subtle" type="text"
        name="fname" placeholder="First Name" required value="<%=editdata.fname %>">
    <input class="w-75 rounded-4  p-2 d-block m-auto mt-1 border border-secondary-subtle" type="text"
        name="lname" placeholder="Last Name" required value="<%=editdata.lname %>">
    <input class="w-75 rounded-4  p-2 d-block m-auto mt-1 border border-secondary-subtle" type="email"
        name="emailid" placeholder="Email" required value="<%=editdata.emailid %>">
    <input class="w-75 rounded-4  p-2 d-block m-auto mt-1 border border-secondary-subtle"
        type="password" name="createpass" placeholder="Create Your Password" required value="<%=editdata.createpass %>">
    <input class="w-75 rounded-4  p-2 d-block m-auto mt-1 border border-secondary-subtle"
        type="password" name="confirmpass" placeholder="Confirm Your Password" required value="<%=editdata.confirmpass %>">
    <button
        class="btn btn-dark w-75 rounded-4 p-3 d-block m-auto mt-1 border border-secondary-subtle">Edit</button>
    <!-- <h6 class="text-center mt-2">Have an account? <a class="text-danger" href="/login"> Log In</a></h6> -->
</form>